/* Timeline Elements */

#container {
  position: absolute;
  left: 0px;
  top: 710px;
  width: 100%;
  background-color: #f6f6f6;
}

  .timeline {
    list-style: none;
    padding: 50px 0 20px;
    position: relative;
  }
  
  .timeline:before {
    top: 0;
    bottom: 0;
    position: absolute;
    content: " ";
    width: 3px;
    background-color: #eeeeee;
    left: 50%;
    margin-left: -1.5px;
  }
  
  .timeline > li {
    margin-bottom: 20px;
    position: relative;
    width: 50%;
    float: left;
    clear: left;
  }
    
    .timeline > li:before,
    .timeline > li:after {
      content: " ";
      display: table;
    }

    .timeline > li:after {
      clear: both;
    }

    .timeline > li.timeline-inverted{
      float: right; 
      clear: right;
      margin-top: 100px;
      margin-bottom: 30px;
    }

